<h1>Listing projects_and_concepts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phase</th>
      <th>Ba</th>
      <th>Pm</th>
      <th>Next phase</th>
      <th>Next phase due</th>
      <th>Original start date</th>
      <th>Project end date</th>
      <th>Continuing ba commitment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects_and_concepts.each do |projects_and_concept| %>
      <tr>
        <td><%= projects_and_concept.Name %></td>
        <td><%= projects_and_concept.Phase %></td>
        <td><%= projects_and_concept.BA %></td>
        <td><%= projects_and_concept.PM %></td>
        <td><%= projects_and_concept.Next_Phase %></td>
        <td><%= projects_and_concept.Next_Phase_Due %></td>
        <td><%= projects_and_concept.Original_Start_Date %></td>
        <td><%= projects_and_concept.Project_End_Date %></td>
        <td><%= projects_and_concept.Continuing_BA_Commitment %></td>
        <td><%= link_to 'Show', projects_and_concept %></td>
        <td><%= link_to 'Edit', edit_projects_and_concept_path(projects_and_concept) %></td>
        <td><%= link_to 'Destroy', projects_and_concept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

